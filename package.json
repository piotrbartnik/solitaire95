{
  "name": "windows95-solitaire-monorepo",
  "version": "0.1.2",
  "private": true,
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "scripts": {
    "install": "lerna bootstrap",
    "start:web": "cd ./applications/webApp && yarn start",
    "start:desktop": "cd ./applications/electron-react-app && yarn start",
    "rmmodules": "find . -name 'node_modules' -type d -exec rm -rf '{}' +",
    "build:packages": "lerna run webpack"
  }
}
